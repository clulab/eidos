
GET  /eidos/v1/ping           controllers.v1.HomeController.ping
POST /eidos/v1/echo           controllers.v1.HomeController.echo(text: String)

GET  /eidos/v2/status         controllers.v2.HomeController.status
POST /eidos/v2/readText       controllers.v2.HomeController.read(text: String, title: Option[String], id: Option[String], date: Option[String], location: Option[String])
#POST /eidos/v2/readCdr        controllers.v2.HomeController.read(text: String)

# Map static resources from the /public folder to the /assets URL path
GET  /eidos/assets/*file            controllers.Assets.versioned(path="/public", file: Asset)
GET  /favicon.ico             controllers.Assets.at(path="/public", file="/images/favicon.ico")
